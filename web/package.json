  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PheonBest/vite-react-ts-shadcn-ui-ci-cd-devbox"
  },
  "issues": {
    "url": "https://github.com/PheonBest/vite-react-ts-shadcn-ui-ci-cd-devbox/issues"
  },
  "homepage": "/launchplate-react",
  "scripts": {
    "prepare": "cd .. && husky install front/.husky",
    "postinstall": "husky & playwright install",
    "routes:watch": "tsr watch",
    "routes:generate": "tsr generate",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "test": "vitest --coverage",
    "test:e2e": "cross-env VITE_COVERAGE=true playwright test",
    "test:watch": "vitest --watch",
    "test:ui": "playwright test --ui",
    "build:production": "vite build",
    "build:qa": "vite build --mode qa",
    "build:staging": "vite build --mode staging",
    "build": "vite build",
    "publish:qa": "aws s3 cp dist s3://qa-launchplate-react - recursive",
    "publish:staging": "aws s3 cp dist s3://staging-launchplate-react - recursive",
    "commit": "git-cz",
    "betterer": "betterer",
    "betterer:watch": "betterer --watch",
    "betterer:ci": "betterer ci",
    "serve": "tsx dist-server/server.ts"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "ignorePattern": "node_modules/**",
    "src/**/*.{html,css,scss,ts,tsx}": "npx prettier --write",
    "src/**/*.{ts,tsx}": "npx eslint --fix"
  },
  "engines": {
    "node": ">=v18",
    "pnpm": ">=8.0.0"
  },
  "keywords": [
    "vite",
    "react",
    "typescript",
    "shadcn-ui",
    "tailwindcss",
    "radix-ui",
    "zustand",
    "tanstack-router"
  ],
  "browserslist": [
    "last 3 versions",
    "> 0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
